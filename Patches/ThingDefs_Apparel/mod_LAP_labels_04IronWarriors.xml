<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - armour                                                                                         -->
<!--       + For LAP_Armour_IronWarriors_Armor REPLACE label                                                          -->
<!--                                              WITH label Iron Warriors Power Armour                               -->
<!--       + For LAP_Armour_IronWarriors_JumpPack REPLACE label                                                       -->
<!--                                                 WITH label Iron Warriors Assault Armour                          -->
<!--       + For LAP_Armour_IronWarriors_Apothecary_Armor REPLACE label                                               -->
<!--                                                         WITH label Iron Warriors Apothecary Armour               -->
<!--       + For LAP_Armour_IronWarriors_Techmarine_Armor REPLACE label                                               -->
<!--                                                         WITH label Iron Warriors Techmarine Armour               -->
<!--       + For LAP_Armour_Perturabo_Armor REPLACE label                                                             -->
<!--                                           WITH label the Logos                                                   -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - helmets                                                                                        -->
<!--       + For LAP_Armour_Perturabo_Head REPLACE label                                                              -->
<!--                                          WITH label the Logos' Support System                                    -->
<!--  ==============================================================================================================  -->

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Legiones Astartes: Primarch</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - armour                                                                                         -->
<!--  ==============================================================================================================  -->
<!--       + For LAP_Armour_IronWarriors_Armor REPLACE label                                                          -->
<!--                                              WITH label Iron Warriors Power Armour                               -->
<!--       + For LAP_Armour_IronWarriors_JumpPack REPLACE label                                                       -->
<!--                                                 WITH label Iron Warriors Assault Armour                          -->
<!--       + For LAP_Armour_IronWarriors_Apothecary_Armor REPLACE label                                               -->
<!--                                                         WITH label Iron Warriors Apothecary Armour               -->
<!--       + For LAP_Armour_IronWarriors_Techmarine_Armor REPLACE label                                               -->
<!--                                                         WITH label Iron Warriors Techmarine Armour               -->
<!--       + For LAP_Armour_Perturabo_Armor REPLACE label                                                             -->
<!--                                           WITH label the Logos                                                   -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="LAP_Armour_IronWarriors_Armor"]/label</xpath>
                    <value>
                        <label>Iron Warriors Power Armour</label>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="LAP_Armour_IronWarriors_JumpPack"]/label</xpath>
                    <value>
                        <label>Iron Warriors Assault Armour</label>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="LAP_Armour_IronWarriors_Apothecary_Armor"]/label</xpath>
                    <value>
                        <label>Iron Warriors Apothecary Armour</label>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="LAP_Armour_IronWarriors_Techmarine_Armor"]/label</xpath>
                    <value>
                        <label>Iron Warriors Techmarine Armour</label>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="LAP_Armour_Perturabo_Armor"]/label</xpath>
                    <value>
                        <label>the Logos</label>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - helmets                                                                                        -->
<!--  ==============================================================================================================  -->
<!--       + For LAP_Armour_Perturabo_Head REPLACE label                                                              -->
<!--                                          WITH label the Logos' Support System                                    -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="LAP_Armour_Perturabo_Head"]/label</xpath>
                    <value>
                        <label>the Logos' Support System</label>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
            </operations>
        </match>
    </Operation>
</Patch>
