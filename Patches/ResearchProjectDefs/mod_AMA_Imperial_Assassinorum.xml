<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES  (ResearchProjectDef)  -  OfficioAssassinorum                                                       -->
<!--    + OG_Assassinorum_Tech_Base REPLACE description                                                               -->
<!--                                   WITH description  Grants access to the Officio Assassinorum.                   -->
<!--    + OG_Assassinorum_Tech_Base REPLACE researchViewX                                                             -->
<!--                                   WITH researchViewX  7                                                          -->
<!--    + OG_Assassinorum_Tech_Base REPLACE researchViewY                                                             -->
<!--                                   WITH researchViewY  2.55                                                       -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES  (ResearchProjectDef)  -  CallidusTemple                                                            -->
