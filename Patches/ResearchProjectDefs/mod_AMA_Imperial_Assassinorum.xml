                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/label</xpath>
                    <value>
                        <label>Callidus temple</label>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/description</xpath>
                    <value>
                        <description>Grants access to the STC templates unique to the Callidus temple of the Officio Assassinorum.</description>
                    </value>
                </li>

                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/heldByFactionCategoryTags</xpath>
                    <value>
                        <li>Empire</li>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/researchViewX</xpath>
                    <value>
                        <researchViewX>8</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/researchViewY</xpath>
                    <value>
                        <researchViewY>2.35</researchViewY>
                    </value>
                </li>

