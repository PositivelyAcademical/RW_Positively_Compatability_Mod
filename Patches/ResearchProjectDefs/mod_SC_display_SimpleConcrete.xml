<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL CASES                                                                                                  -->
<!--       + For WDReinforcedConcrete REPLACE researchViewY (5.3)                                                     -->
<!--                                     WITH researchViewY 4.8                                                       -->
<!--  ==============================================================================================================  -->

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[WD] Simple Concrete</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--     + ALL CASES                                                                                                  -->
<!--  ==============================================================================================================  -->
<!--       + For WDReinforcedConcrete REPLACE researchViewY (5.3)                                                     -->
<!--                                     WITH researchViewY 4.8                                                       -->
<!--  ==============================================================================================================  -->
                <li Class = "PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="WDReinforcedConcrete"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.8</researchViewY>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>
</Patch>
