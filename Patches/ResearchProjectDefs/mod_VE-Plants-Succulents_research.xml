<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 4 (Y = 1.86)                                                             -->
<!--    + VCE_SucculentSowing IGNORE researchViewX                                                                    -->
<!--    + VCE_SucculentSowing REPLACE researchViewY                                                                   -->
<!--                             WITH researchViewY  1.86                                                             -->
<!--  ==============================================================================================================  -->

    <Operation Class="PatchOperationFindMod">
        <mods>
        <li>Vanilla Plants Expanded - Succulents</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 4 (Y = 1.86)                                                             -->
<!--  ==============================================================================================================  -->
<!--    + VCE_SucculentSowing REPLACE researchViewY                                                                   -->
<!--                             WITH researchViewY  1.86                                                             -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="VCE_SucculentSowing"]/researchViewY</xpath>
                    <value>
                        <researchViewY>1.86</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
            </operations>
        </match>
    </Operation>

</Patch>
