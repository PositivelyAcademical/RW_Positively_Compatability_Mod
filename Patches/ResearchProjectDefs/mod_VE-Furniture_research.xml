<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 8 (Y = 4.34)                                                             -->
<!--    + MF_ModernFurniture REPLACE researchViewX                                                                    -->
<!--                            WITH researchViewX  3                                                                 -->
<!--    + MF_ModernFurniture REPLACE researchViewY                                                                    -->
<!--                            WITH researchViewY  4.34                                                              -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 9 (Y = 4.96)                                                             -->
<!--    + MF_BasicFurniture REPLACE researchViewX                                                                     -->
<!--                           WITH researchViewX  1                                                                  -->
<!--    + MF_BasicFurniture REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  4.96                                                               -->
<!--    + ComplexFurniture REPLACE researchViewX                                                                      -->
<!--                          WITH researchViewX  2                                                                   -->
<!--    + ComplexFurniture REPLACE researchViewY                                                                      -->
<!--                          WITH researchViewY  4.96                                                                -->
<!--    + MF_RoyalFurniture REPLACE researchViewX                                                                     -->
<!--                           WITH researchViewX  3                                                                  -->
<!--    + MF_RoyalFurniture REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  4.96                                                               -->
<!--    + MF_WasteDisposal REPLACE researchViewX                                                                      -->
<!--                          WITH researchViewX  4                                                                   -->
<!--    + MF_WasteDisposal REPLACE researchViewY                                                                      -->
<!--                          WITH researchViewY  4.96                                                                -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 10 (Y = 5.58)                                                            -->
<!--    + TubeTelevision IGNORE researchViewX                                                                         -->
<!--    + TubeTelevision REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  5.58                                                                  -->
<!--    + FlatscreenTelevision IGNORE researchViewX                                                                   -->
<!--    + FlatscreenTelevision REPLACE researchViewY                                                                  -->
<!--                              WITH researchViewY  5.58                                                            -->
<!--  ==============================================================================================================  -->

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 8 (Y = 4.34)                                                             -->
<!--  ==============================================================================================================  -->
<!--    + MF_ModernFurniture REPLACE researchViewX                                                                    -->
<!--                            WITH researchViewX  3                                                                 -->
<!--    + MF_ModernFurniture REPLACE researchViewY                                                                    -->
<!--                            WITH researchViewY  4.34                                                              -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_ModernFurniture"]/researchViewX</xpath>
                    <value>
                        <researchViewX>3</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_ModernFurniture"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.34</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 9 (Y = 4.96)                                                             -->
<!--  ==============================================================================================================  -->
<!--    + MF_BasicFurniture REPLACE researchViewX                                                                     -->
<!--                           WITH researchViewX  1                                                                  -->
<!--    + MF_BasicFurniture REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  4.96                                                               -->
<!--    + ComplexFurniture REPLACE researchViewX                                                                      -->
<!--                          WITH researchViewX  2                                                                   -->
<!--    + ComplexFurniture REPLACE researchViewY                                                                      -->
<!--                          WITH researchViewY  4.96                                                                -->
<!--    + MF_RoyalFurniture REPLACE researchViewX                                                                     -->
<!--                           WITH researchViewX  3                                                                  -->
<!--    + MF_RoyalFurniture REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  4.96                                                               -->
<!--    + MF_WasteDisposal REPLACE researchViewX                                                                      -->
<!--                          WITH researchViewX  4                                                                   -->
<!--    + MF_WasteDisposal REPLACE researchViewY                                                                      -->
<!--                          WITH researchViewY  4.96                                                                -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_BasicFurniture"]/researchViewX</xpath>
                    <value>
                        <researchViewX>1</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_BasicFurniture"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.96</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="ComplexFurniture"]/researchViewX</xpath>
                    <value>
                        <researchViewX>2</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="ComplexFurniture"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.96</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_RoyalFurniture"]/researchViewX</xpath>
                    <value>
                        <researchViewX>3</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_RoyalFurniture"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.96</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_WasteDisposal"]/researchViewX</xpath>
                    <value>
                        <researchViewX>4</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MF_WasteDisposal"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.96</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 10 (Y = 5.58)                                                            -->
<!--  ==============================================================================================================  -->
<!--    + TubeTelevision REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  5.58                                                                  -->
<!--    + FlatscreenTelevision REPLACE researchViewY                                                                  -->
<!--                              WITH researchViewY  5.58                                                            -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="TubeTelevision"]/researchViewY</xpath>
                    <value>
                        <researchViewY>5.58</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="FlatscreenTelevision"]/researchViewY</xpath>
                    <value>
                        <researchViewY>5.58</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
            </operations>
        </match>
    </Operation>

</Patch>
