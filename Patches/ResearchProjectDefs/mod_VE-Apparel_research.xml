                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="VAE_MilitaryClothing"]/researchViewY</xpath>
                    <value>
                        <researchViewY>1.86</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
            </operations>
        </match>
