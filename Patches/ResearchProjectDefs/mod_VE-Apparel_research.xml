<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 3 (Y = 1.24)                                                             -->
<!--    + VAE_CasualWear IGNORE researchViewX                                                                         -->
<!--    + VAE_CasualWear REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.24                                                                  -->
<!--    + VAE_FormalWear IGNORE researchViewX                                                                         -->
<!--    + VAE_FormalWear REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.24                                                                  -->
<!--    + VAE_WorkAttire REPLACE researchViewX                                                                        -->
<!--                        WITH researchViewX  2                                                                     -->
<!--    + VAE_WorkAttire REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.24                                                                  -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 4 (Y = 1.86)                                                             -->
