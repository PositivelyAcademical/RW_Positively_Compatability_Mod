<!--                           WITH researchViewX  2                                                                  -->
<!--    + VAE_SterileAttire REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  1.86                                                               -->
<!--    + VAE_MilitaryClothing IGNORE researchViewX                                                                   -->
<!--    + VAE_MilitaryClothing REPLACE researchViewY                                                                  -->
<!--                              WITH researchViewY  1.86                                                            -->
<!--  ==============================================================================================================  -->

    <Operation Class="PatchOperationFindMod">
        <mods>
        <li>Vanilla Apparel Expanded</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 3 (Y = 1.24)                                                             -->
<!--  ==============================================================================================================  -->
<!--    + VAE_CasualWear REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.24                                                                  -->
<!--    + VAE_FormalWear REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.24                                                                  -->
<!--    + VAE_WorkAttire REPLACE researchViewX                                                                        -->
<!--                        WITH researchViewX  2                                                                     -->
<!--    + VAE_WorkAttire REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.24                                                                  -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="VAE_CasualWear"]/researchViewY</xpath>
                    <value>
                        <researchViewY>1.24</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="VAE_FormalWear"]/researchViewY</xpath>
                    <value>
                        <researchViewY>1.24</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="VAE_WorkAttire"]/researchViewX</xpath>
