<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - Assassinorum                                                                                   -->
<!--       + For OG_Assassinorum_Tech_Base REPLACE researchViewX 7.00 with 6                                          -->
<!--       + For OG_Assassinorum_Tech_Base REPLACE researchViewY 2.00 with 3.00                                       -->
<!--       + For OG_Assassinorum_Tech_Weapons_Vindicare REPLACE researchViewX 7.75 with 7                             -->
<!--       + For OG_Assassinorum_Tech_Weapons_Vindicare REPLACE researchViewY 1.00 with 2.25                          -->
<!--       + For OG_Assassinorum_Tech_Weapons_Callidus REPLACE researchViewX 7.725 with 7                             -->
<!--       + For OG_Assassinorum_Tech_Weapons_Callidus REPLACE researchViewY 1.50 with 2.75                           -->
<!--       + For OG_Assassinorum_Tech_Weapons_Culexus REPLACE researchViewX 7.725 with 7                              -->
<!--       + For OG_Assassinorum_Tech_Weapons_Culexus REPLACE researchViewY 2.50 with 3.25                            -->
<!--       + For OG_Assassinorum_Tech_Weapons_Eversor REPLACE researchViewX 7.75 with 7                               -->
<!--       + For OG_Assassinorum_Tech_Weapons_Eversor REPLACE researchViewY 3.0 with 3.75                             -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Adeptus Mechanicus: Armoury</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - Assassinorum                                                                                   -->
<!--  ==============================================================================================================  -->
<!--       + For OG_Assassinorum_Tech_Base REPLACE researchViewX 7.00 with 6                                          -->
<!--       + For OG_Assassinorum_Tech_Base REPLACE researchViewY 2.00 with 3.00                                       -->
<!--       + For OG_Assassinorum_Tech_Weapons_Vindicare REPLACE researchViewX 7.75 with 7                             -->
<!--       + For OG_Assassinorum_Tech_Weapons_Vindicare REPLACE researchViewY 1.00 with 2.25                          -->
<!--       + For OG_Assassinorum_Tech_Weapons_Callidus REPLACE researchViewX 7.725 with 8                             -->
<!--       + For OG_Assassinorum_Tech_Weapons_Callidus REPLACE researchViewY 1.50 with 2.75                           -->
<!--       + For OG_Assassinorum_Tech_Weapons_Culexus REPLACE researchViewX 7.725 with 8                              -->
<!--       + For OG_Assassinorum_Tech_Weapons_Culexus REPLACE researchViewY 2.50 with 3.25                            -->
<!--       + For OG_Assassinorum_Tech_Weapons_Eversor REPLACE researchViewX 7.75 with 7                               -->
<!--       + For OG_Assassinorum_Tech_Weapons_Eversor REPLACE researchViewY 3.0 with 3.75                             -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Base"]/researchViewX</xpath>
                    <value>
                        <researchViewX>6</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Base"]/researchViewY</xpath>
                    <value>
                        <researchViewY>3.00</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Vindicare"]/researchViewX</xpath>
                    <value>
                        <researchViewX>7</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Vindicare"]/researchViewY</xpath>
                    <value>
                        <researchViewY>2.25</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/researchViewX</xpath>
                    <value>
                        <researchViewX>7</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Callidus"]/researchViewY</xpath>
                    <value>
                        <researchViewY>2.75</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Culexus"]/researchViewX</xpath>
                    <value>
                        <researchViewX>7</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Culexus"]/researchViewY</xpath>
                    <value>
                        <researchViewY>3.25</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Eversor"]/researchViewX</xpath>
                    <value>
                        <researchViewX>7</researchViewX>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Assassinorum_Tech_Weapons_Eversor"]/researchViewY</xpath>
                    <value>
                        <researchViewY>3.75</researchViewY>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>
</Patch>
