<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 1                                                                                          -->
<!--       + For TreeSowing IGNORE researchViewX                                                                      -->
<!--       + For TreeSowing IGNORE researchViewY                                                                      -->
<!--       + For Cocoa IGNORE researchViewX                                                                           -->
<!--       + For Cocoa IGNORE researchViewY                                                                           -->
<!--       + For DrugProduction IGNORE researchViewX                                                                  -->
<!--       + For DrugProduction IGNORE researchViewY                                                                  -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 2                                                                                          -->
<!--       + For RecurveBow IGNORE researchViewX                                                                      -->
<!--       + For RecurveBow IGNORE researchViewY                                                                      -->
<!--       + For Greatbow IGNORE researchViewX                                                                        -->
<!--       + For Greatbow IGNORE researchViewY                                                                        -->
<!--       + For PsychoidBrewing REPLACE researchViewX                                                                -->
<!--                                WITH researchViewX  2                                                             -->
<!--       + For PsychoidBrewing REPLACE researchViewY                                                                -->
<!--                                WITH researchViewY  0.7                                                           -->
<!--       + For PsychiteRefining IGNORE researchViewX                                                                -->
<!--       + For PsychiteRefining REPLACE researchViewY                                                               -->
<!--                                 WITH researchViewY  0.7                                                          -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 3                                                                                          -->
<!--       + For Brewing IGNORE researchViewX                                                                         -->
<!--       + For Brewing REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.4                                                                   -->
<!--       + For Pemmican IGNORE researchViewX                                                                        -->
<!--       + For Pemmican IGNORE researchViewY                                                                        -->
<!--       + For WakeUpProduction IGNORE researchViewX                                                                -->
<!--       + For WakeUpProduction REPLACE researchViewY                                                               -->
<!--                                 WITH researchViewY  1.4                                                          -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 4                                                                                          -->
<!--       + For PassiveCooler IGNORE researchViewX                                                                   -->
<!--       + For PassiveCooler IGNORE researchViewY                                                                   -->
<!--       + For ComplexClothing IGNORE researchViewX                                                                 -->
<!--       + For ComplexClothing REPLACE researchViewY                                                                -->
<!--                                WITH researchViewY  2.1                                                           -->
<!--       + For GoJuiceProduction IGNORE researchViewX                                                               -->
<!--       + For GoJuiceProduction REPLACE researchViewY                                                              -->
<!--                                  WITH researchViewY  2.1                                                         -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 5                                                                                          -->
<!--       + For Devilstrand IGNORE researchViewX                                                                     -->
<!--       + For Devilstrand IGNORE researchViewY                                                                     -->
<!--       + For CarpetMaking IGNORE researchViewX                                                                    -->
<!--       + For CarpetMaking REPLACE researchViewY                                                                   -->
<!--                             WITH researchViewY  2.8                                                              -->
<!--       + For PlateArmor IGNORE researchViewX                                                                      -->
<!--       + For PlateArmor REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  2.8                                                                -->
<!--       + For LongBlades REPLACE researchViewX                                                                     -->
<!--                           WITH researchViewX  3                                                                  -->
<!--       + For LongBlades REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  2.8                                                                -->
<!--       + For PenoxycylineProduction IGNORE researchViewX                                                          -->
<!--       + For PenoxycylineProduction REPLACE researchViewY                                                         -->
<!--                                       WITH researchViewY  2.8                                                    -->
<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 6                                                                                          -->
<!--       + For Stonecutting IGNORE researchViewX                                                                    -->
<!--       + For Stonecutting REPLACE researchViewY                                                                   -->
<!--                             WITH researchViewY  3.6                                                              -->
<!--       + For Smithing IGNORE researchViewX                                                                        -->
<!--       + For Smithing REPLACE researchViewY                                                                       -->
<!--                         WITH researchViewY  3.6                                                                  -->
<!--  ==============================================================================================================  -->
<!--     + NO_MATCH VanillaFurnitureExpanded - row 7                                                                  -->
<!--       + For ComplexFurniture IGNORE researchViewX                                                                -->
<!--       + For ComplexFurniture REPLACE researchViewY                                                               -->
<!--                                 WITH researchViewY  4.2                                                          -->
<!--  ==============================================================================================================  -->


<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 2                                                                                          -->
<!--  ==============================================================================================================  -->
<!--       + For PsychoidBrewing REPLACE researchViewX                                                                -->
<!--                                WITH researchViewX  2                                                             -->
<!--       + For PsychoidBrewing REPLACE researchViewY                                                                -->
<!--                                WITH researchViewY  0.7                                                           -->
<!--       + For PsychiteRefining REPLACE researchViewY                                                               -->
<!--                                 WITH researchViewY  0.7                                                          -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="PsychoidBrewing"]/researchViewX</xpath>
        <value>
            <researchViewX>2</researchViewX>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="PsychoidBrewing"]/researchViewY</xpath>
        <value>
            <researchViewY>0.7</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="PsychiteRefining"]/researchViewY</xpath>
        <value>
            <researchViewY>0.7</researchViewY>
        </value>
    </Operation>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 3                                                                                          -->
<!--  ==============================================================================================================  -->
<!--       + For Brewing REPLACE researchViewY                                                                        -->
<!--                        WITH researchViewY  1.4                                                                   -->
<!--       + For WakeUpProduction REPLACE researchViewY                                                               -->
<!--                                 WITH researchViewY  1.4                                                          -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="Brewing"]/researchViewY</xpath>
        <value>
            <researchViewY>1.4</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="WakeUpProduction"]/researchViewY</xpath>
        <value>
            <researchViewY>1.4</researchViewY>
        </value>
    </Operation>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 4                                                                                          -->
<!--  ==============================================================================================================  -->
<!--       + For ComplexClothing REPLACE researchViewY                                                                -->
<!--                                WITH researchViewY  2.1                                                           -->
<!--       + For GoJuiceProduction REPLACE researchViewY                                                              -->
<!--                                  WITH researchViewY  2.1                                                         -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="ComplexClothing"]/researchViewY</xpath>
        <value>
            <researchViewY>2.1</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="GoJuiceProduction"]/researchViewY</xpath>
        <value>
            <researchViewY>2.1</researchViewY>
        </value>
    </Operation>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 5                                                                                          -->
<!--  ==============================================================================================================  -->
<!--       + For CarpetMaking REPLACE researchViewY                                                                   -->
<!--                             WITH researchViewY  2.8                                                              -->
<!--       + For PlateArmor REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  2.8                                                                -->
<!--       + For LongBlades REPLACE researchViewX                                                                     -->
<!--                           WITH researchViewX  3                                                                  -->
<!--       + For LongBlades REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  2.8                                                                -->
<!--       + For PenoxycylineProduction REPLACE researchViewY                                                         -->
<!--                                       WITH researchViewY  2.8                                                    -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="CarpetMaking"]/researchViewY</xpath>
        <value>
            <researchViewY>2.8</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="PlateArmor"]/researchViewY</xpath>
        <value>
            <researchViewY>2.8</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="LongBlades"]/researchViewX</xpath>
        <value>
            <researchViewX>3</researchViewX>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="LongBlades"]/researchViewY</xpath>
        <value>
            <researchViewY>2.8</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="PenoxycylineProduction"]/researchViewY</xpath>
        <value>
            <researchViewY>2.8</researchViewY>
        </value>
    </Operation>

<!--  ==============================================================================================================  -->
<!--     + ALL_CASES - row 6                                                                                          -->
<!--  ==============================================================================================================  -->
<!--       + For Stonecutting REPLACE researchViewY                                                                   -->
<!--                             WITH researchViewY  3.6                                                              -->
<!--       + For Smithing REPLACE researchViewY                                                                       -->
<!--                         WITH researchViewY  3.6                                                                  -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="Stonecutting"]/researchViewY</xpath>
        <value>
            <researchViewY>3.6</researchViewY>
        </value>
    </Operation>

    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ResearchProjectDef[defName="Smithing"]/researchViewY</xpath>
        <value>
            <researchViewY>3.6</researchViewY>
        </value>
    </Operation>

<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded</li>
        </mods>
        <nomatch Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--     + NO_MATCH VanillaFurnitureExpanded - row 7                                                                  -->
<!--  ==============================================================================================================  -->
<!--       + For ComplexFurniture REPLACE researchViewY                                                               -->
<!--                                 WITH researchViewY  4.2                                                          -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="ComplexFurniture"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.2</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
            </operations>
        </nomatch>
    </Operation>

</Patch>
