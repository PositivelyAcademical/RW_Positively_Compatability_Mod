<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 9 (Y = 4.96)                                                             -->
<!--    + SpacerFurniture IGNORE researchViewX                                                                        -->
<!--    + SpacerFurniture REPLACE researchViewY                                                                       -->
<!--                         WITH researchViewY  4.96                                                                 -->
<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 10 (Y = 5.58)                                                            -->
<!--    + SpacerElectronics IGNORE researchViewX                                                                      -->
<!--    + SpacerElectronics REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  5.58                                                               -->
<!--    + MechaniteRestoration IGNORE researchViewX                                                                   -->
<!--    + MechaniteRestoration REPLACE researchViewY                                                                  -->
<!--                              WITH researchViewY  5.58                                                            -->
<!--  ==============================================================================================================  -->

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Spacer Module</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 9 (Y = 4.96)                                                             -->
<!--  ==============================================================================================================  -->
<!--    + SpacerFurniture REPLACE researchViewY                                                                       -->
<!--                         WITH researchViewY  4.96                                                                 -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="SpacerFurniture"]/researchViewY</xpath>
                    <value>
                        <researchViewY>4.96</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
<!--  + ALL_CASES (ResearchProjectDef) - row 10 (Y = 5.58)                                                            -->
<!--  ==============================================================================================================  -->
<!--    + SpacerElectronics REPLACE researchViewY                                                                     -->
<!--                           WITH researchViewY  5.58                                                               -->
<!--    + MechaniteRestoration REPLACE researchViewY                                                                  -->
<!--                              WITH researchViewY  5.58                                                            -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="SpacerElectronics"]/researchViewY</xpath>
                    <value>
                        <researchViewY>5.58</researchViewY>
                    </value>
                </li>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="MechaniteRestoration"]/researchViewY</xpath>
                    <value>
                        <researchViewY>5.58</researchViewY>
                    </value>
                </li>

<!--  ==============================================================================================================  -->
            </operations>
        </match>
    </Operation>

</Patch>
