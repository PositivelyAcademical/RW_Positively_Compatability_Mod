				                        <muzzleFlashScale>10</muzzleFlashScale>
				                        <barrelLength>0.35</barrelLength>
				                        <barrelOffset>0.0</barrelOffset>
				                        <targetParams>
					                        <canTargetLocations>true</canTargetLocations>
				                        </targetParams>
