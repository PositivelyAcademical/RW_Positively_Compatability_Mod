<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL CASES                                                                                                  -->
<!--       + For OG_Imperial_Tech_Apparel_Armour_T1 REPLACE label Flakk Armour with label Imperial Flak Armour        -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Adeptus Mechanicus: Armoury</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>


<!--  ==============================================================================================================  -->
<!--     + ALL CASES                                                                                                  -->
<!--  ==============================================================================================================  -->
<!--       + For OG_Imperial_Tech_Apparel_Armour_T1 REPLACE label Flakk Armour with label Imperial Flak Armour        -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ResearchProjectDef[defName="OG_Imperial_Tech_Apparel_Armour_T1"]/label</xpath>
                    <value>
                        <label>Imperial Flak Armour</label>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>
</Patch>
