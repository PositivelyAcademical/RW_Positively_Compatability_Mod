<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL CASES - Imperial                                                                                       -->
<!--       + For @OG_Imperial_Apparel_Armor_Makeable REMOVE researchPrerequisites                                     -->
<!--           (OG_Imperial_Tech_Apparel_Armour_T1)                                                                   -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Adeptus Mechanicus: Armoury</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>


<!--  ==============================================================================================================  -->
<!--     + ALL CASES                                                                                                  -->
<!--  ==============================================================================================================  -->
<!--       + For @OG_Imperial_Apparel_Armor_Makeable REMOVE researchPrerequisites                                     -->
<!--           (OG_Imperial_Tech_Apparel_Armour_T1)                                                                   -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationRemove">
                    <xpath>/Defs/ThingDef[@Name="OG_Imperial_Apparel_Armor_Makeable"]/recipeMaker/researchPrerequisites</xpath>
                </li>












            </operations>
        </match>
    </Operation>
</Patch>
