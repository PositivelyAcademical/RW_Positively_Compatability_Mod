<?xml version="1.0" encoding="UTF-8"?>
<Patch>

<!--  ==============================================================================================================  -->
<!--  What this xml patch does:                                                                                       -->
<!--  ==============================================================================================================  -->
<!--     + ALL CASES - MilitarumHeadgear                                                                              -->
<!--       + For OGIG_Apparel_GasMask INSERT researchPrerequisites (OG_Imperial_Tech_Apparel) after                   -->
<!--           generateCommonality                                                                                    -->
<!--  ==============================================================================================================  -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Adeptus Mechanicus: Armoury</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

<!--  ==============================================================================================================  -->
<!--     + ALL CASES - MilitarumHeadgear                                                                              -->
<!--  ==============================================================================================================  -->
<!--       + For OGIG_Apparel_GasMask INSERT researchPrerequisites (OG_Imperial_Tech_Apparel) after                   -->
<!--           generateCommonality                                                                                    -->
<!--  ==============================================================================================================  -->
                <li Class="PatchOperationInsert">
                    <xpath>/Defs/ThingDef[defName="OGIG_Apparel_GasMask"]/generateCommonality</xpath>
                    <value>
                        <recipeMaker>
                            <researchPrerequisites>
                                <li>OG_Imperial_Tech_Apparel</li>
                            </researchPrerequisites>
                        </recipeMaker>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>
</Patch>
